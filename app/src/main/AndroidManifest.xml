<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.ukdw.woney">

    <!-- boleh mengunakan internet -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_apk"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_apk_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login2Activity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".CobaActivity" />
        <activity android:name=".TransferActivity" />
        <activity android:name=".CekTransferActivity" />
        <activity android:name=".ChangeNameActivity" />
        <activity android:name=".ChangeEmailActivity" />
        <activity android:name=".ChangePasswordActivity" />
        <activity android:name=".ChangePINActivity"></activity>
    </application>

</manifest>